<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/iv_lv"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_margin="30dp"
        android:scaleType="centerCrop"
        android:src="@drawable/img_mountain"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_lv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/dialog_lv"
        android:layout_marginStart="20dp"
        android:textSize="24sp"
        android:textColor="@color/offroader_green"
        android:fontFamily="@font/bm_hanna_11yrs_old"
        app:layout_constraintTop_toTopOf="@id/iv_lv"
        app:layout_constraintBottom_toBottomOf="@id/iv_lv"
        app:layout_constraintStart_toEndOf="@id/iv_lv"/>

    <ImageView
        android:id="@+id/iv_lv1"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="120dp"
        android:src="@drawable/ic_weather"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_lv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="@string/dialog_lv1"
        android:textSize="12sp"
        android:layout_marginStart="30dp"
        android:fontFamily="@font/pretendard_medium"
        app:layout_constraintStart_toEndOf="@id/iv_lv1"
        app:layout_constraintTop_toTopOf="@id/iv_lv1"/>

    <TextView
        android:id="@+id/tv_lv1_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/dialog_lv1_title"
        android:textSize="16sp"
        android:layout_marginStart="30dp"
        android:fontFamily="@font/pretendard_medium"
        android:textColor="@color/offroader_green"
        app:layout_constraintStart_toEndOf="@id/iv_lv1"
        app:layout_constraintBottom_toBottomOf="@id/iv_lv1"/>

    <ImageView
        android:id="@+id/iv_lv2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="30dp"
        android:layout_marginStart="30dp"
        android:src="@drawable/ic_lv2"
        app:layout_constraintTop_toBottomOf="@id/iv_lv1"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/tv_lv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="@string/dialog_lv2"
        android:textSize="12sp"
        android:layout_marginStart="30dp"
        android:fontFamily="@font/pretendard_medium"
        app:layout_constraintStart_toEndOf="@id/iv_lv2"
        app:layout_constraintTop_toTopOf="@id/iv_lv2"/>

    <TextView
        android:id="@+id/tv_lv2_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/dialog_lv2_title"
        android:textSize="16sp"
        android:layout_marginStart="30dp"
        android:fontFamily="@font/pretendard_medium"
        android:textColor="@color/offroader_green"
        app:layout_constraintStart_toEndOf="@id/iv_lv2"
        app:layout_constraintBottom_toBottomOf="@id/iv_lv2"/>

    <ImageView
        android:id="@+id/iv_lv3"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="30dp"
        android:layout_marginStart="30dp"
        android:src="@drawable/ic_lv3"
        app:layout_constraintTop_toBottomOf="@id/iv_lv2"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/tv_lv3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="@string/dialog_lv3"
        android:textSize="12sp"
        android:layout_marginStart="30dp"
        android:fontFamily="@font/pretendard_medium"
        app:layout_constraintStart_toEndOf="@id/iv_lv3"
        app:layout_constraintTop_toTopOf="@id/iv_lv3"/>

    <TextView
        android:id="@+id/tv_lv3_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="등산러"
        android:textSize="16sp"
        android:layout_marginStart="30dp"
        android:fontFamily="@font/pretendard_medium"
        android:textColor="@color/offroader_green"
        app:layout_constraintStart_toEndOf="@id/iv_lv3"
        app:layout_constraintBottom_toBottomOf="@id/iv_lv3"/>

    <ImageView
        android:id="@+id/iv_lv4"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="30dp"
        android:layout_marginStart="30dp"
        android:src="@drawable/ic_lv4"
        app:layout_constraintTop_toBottomOf="@id/iv_lv3"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/tv_lv4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Lv. 4"
        android:textSize="12sp"
        android:layout_marginStart="30dp"
        android:fontFamily="@font/pretendard_medium"
        app:layout_constraintStart_toEndOf="@id/iv_lv4"
        app:layout_constraintTop_toTopOf="@id/iv_lv4"/>

    <TextView
        android:id="@+id/tv_lv4_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="프로 등산러"
        android:textSize="16sp"
        android:layout_marginStart="30dp"
        android:fontFamily="@font/pretendard_medium"
        android:textColor="@color/offroader_green"
        app:layout_constraintStart_toEndOf="@id/iv_lv4"
        app:layout_constraintBottom_toBottomOf="@id/iv_lv4"/>

    <ImageView
        android:id="@+id/iv_lv5"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="30dp"
        android:layout_marginStart="30dp"
        android:src="@drawable/ic_lv5"
        app:layout_constraintTop_toBottomOf="@id/iv_lv4"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/tv_lv5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Lv. 5"
        android:textSize="12sp"
        android:layout_marginStart="30dp"
        android:fontFamily="@font/pretendard_medium"
        app:layout_constraintStart_toEndOf="@id/iv_lv5"
        app:layout_constraintTop_toTopOf="@id/iv_lv5"/>

    <TextView
        android:id="@+id/tv_lv5_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="산신령"
        android:textSize="16sp"
        android:layout_marginStart="30dp"
        android:fontFamily="@font/pretendard_medium"
        android:textColor="@color/offroader_green"
        app:layout_constraintStart_toEndOf="@id/iv_lv5"
        app:layout_constraintBottom_toBottomOf="@id/iv_lv5"/>

    <TextView
        android:id="@+id/tv_achive"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="80dp"
        android:hint="업적(개수)"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/tv_achive_lv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0~9"
        android:textSize="16sp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/pretendard_semibold"
        app:layout_constraintTop_toBottomOf="@id/tv_achive"
        app:layout_constraintStart_toStartOf="@id/tv_achive"/>

    <TextView
        android:id="@+id/tv_achive_lv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="10~29"
        android:textSize="16sp"
        android:layout_marginTop="58dp"
        android:fontFamily="@font/pretendard_semibold"
        app:layout_constraintTop_toBottomOf="@id/tv_achive_lv1"
        app:layout_constraintStart_toStartOf="@id/tv_achive_lv1"/>

    <TextView
        android:id="@+id/tv_achive_lv3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="30~69"
        android:textSize="16sp"
        android:layout_marginTop="58dp"
        android:fontFamily="@font/pretendard_semibold"
        app:layout_constraintTop_toBottomOf="@id/tv_achive_lv2"
        app:layout_constraintStart_toStartOf="@id/tv_achive_lv2"/>

    <TextView
        android:id="@+id/tv_achive_lv4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="70~149"
        android:textSize="16sp"
        android:layout_marginTop="58dp"
        android:fontFamily="@font/pretendard_semibold"
        app:layout_constraintTop_toBottomOf="@id/tv_achive_lv3"
        app:layout_constraintStart_toStartOf="@id/tv_achive_lv3"/>

    <TextView
        android:id="@+id/tv_achive_lv5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="150~"
        android:textSize="16sp"
        android:layout_marginTop="58dp"
        android:fontFamily="@font/pretendard_semibold"
        app:layout_constraintTop_toBottomOf="@id/tv_achive_lv4"
        app:layout_constraintStart_toStartOf="@id/tv_achive_lv4"/>

</androidx.constraintlayout.widget.ConstraintLayout>